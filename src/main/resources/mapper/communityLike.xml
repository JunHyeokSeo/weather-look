<?xml version="1.0" encoding="UTF-8" ?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="commlike">
 
	<insert id="linsert" parameterType="commlike">
	insert into community_like values(community_like_seq.nextval,#{post_id},#{nick})
	</insert>

	<select id = "like" parameterType="int" resultType="commlike">
		select * from community_like where post_id =#{post_id}
	</select>
	
</mapper>

	


